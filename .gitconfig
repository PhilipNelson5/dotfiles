  name = Philip Nelson
[log]
  decorate = true

[alias]
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %Creset' --abbrev-commit --date=relative
  ls = ls-tree --full-tree -r --name-only HEAD
  fresh = commit -am
  co = checkout
  ci = commit
  stat = status
  s = status
[color]
  ui = auto
[pager]
	branch = false
[core]
	autocrlf = input
